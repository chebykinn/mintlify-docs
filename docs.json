{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "SourceWizard",
  "colors": {
    "primary": "#0000FF",
    "dark": "#000077"
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "quickstart",
              "maintainers-quickstart"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "href": "https://sourcewizard.ai/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://sourcewizard.ai/dashboard"
    }
  },
  "contextual": {
    "options": [
     "claude",
     "mcp",
     "cursor",
     "cli"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/sourcewizardai",
      "github": "https://github.com/sourcewizard-ai"
    }
  }
}
